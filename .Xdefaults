! urxvt.font:             xft:Terminus:pixelsize=18
!urxvt.backround:        black
!urxvt.foreground:       green

!urxvt.geometry:         80x24+0+14
urxvt.shading:          30
urxvt.depth:            24
urxvt.borderLess:	    false
urxvt.internalBorder:	2
urxvt.inheritPixmap:    true
urxvt.saveLines:        5000
urxvt.scrollBar:        false
urxvt.termName:         rxvt
urxvt.underlineColor:   #86a2be

urxvt.perl-lib::            /usr/lib/urxvt/perl/
urxvt.perl-ext-common:      matcher,searchable-scrollback
urxvt.matcher.button:       1
urxvt.matcher.pattern.1:    \bwww\.[\w-]\.[\w./?&@#-].[\w/-]
urxvt.urlLauncher:          rxvtOpera

urxvt.foreground:     rgb:ee/ee/ee
urxvt.background:     rgb:10/10/10


urxvt.keysym.Control-Up:   \033[1;5A
urxvt.keysym.Control-Down:    \033[1;5B
urxvt.keysym.Control-Left:    \033[1;5D
urxvt.keysym.Control-Right:    \033[1;5C

! }}}

